{
    "extends": "@vue/tsconfig/tsconfig.dom.json",
    "include": [
        "env.d.ts",
        "src/**/*",
        "src/**/*.vue",
        "src/**/*.ts",
        "src/**/**/*.ts"
    ],
    "exclude": [
        "dist",
        "node_modules",
        "src/**/__tests__/*"
    ],
    "compilerOptions": {
        "composite": true,
        "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
        "baseUrl": ".",
        "types": [
            "./auto-imports.d.ts",
            "./node_modules/bootstrap-icons-vue/dist/icons.d.ts",
            "./components.d.ts"
        ],
        "paths": {
            "@/*": [
                "./src/*"
            ],
            "@/models/*": [
                "src/models/*"
            ],
            "@/components/*": [
                "./src/components/*"
            ],
            "@/utils/*": [
                "./src/utils/*"
            ],
        }
    }
}